<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.bluetooth.printer.view.home.HomeFragment"
        android:label="@string/home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_view_house"
        android:name="com.bluetooth.printer.view.home.ViewFragment"
        android:label="@string/house_details"
        tools:layout="@layout/fragment_view_details" />


    <fragment
        android:id="@+id/nav_my_house"
        android:name="com.bluetooth.printer.view.myhouse.MyHouseFragment"
        android:label="@string/my_flat"
        tools:layout="@layout/fragment_my_house" />

    <fragment
        android:id="@+id/nav_edit_house"
        android:name="com.bluetooth.printer.view.myhouse.EditFragment"
        android:label="@string/house_details"
        tools:layout="@layout/fragment_edit_details" />


    <fragment
        android:id="@+id/nav_about_us"
        android:name="com.bluetooth.printer.view.AboutUsFragment"
        android:label="@string/about_us"
        tools:layout="@layout/fragment_web_view" />

